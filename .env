# Toggle between local and production
# Set to 'true' to force localhost, 'false' to force production, or leave unset for auto-detect
VITE_USE_LOCAL=false

# Local development URLs (used when VITE_USE_LOCAL=true)
VITE_API_URL_LOCAL=http://localhost:8005
VITE_WS_URL_LOCAL=ws://localhost:8005/ws

# Production URLs (used when VITE_USE_LOCAL=false)
VITE_API_URL=https://ts-int.digital
VITE_WS_URL=ws://ts-int.digital/ws
